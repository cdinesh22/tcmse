{
  "version": 2,
  "builds": [
    { "src": "frontend/api/**/*.js", "use": "@vercel/node" },
    { "src": "frontend/index.html", "use": "@vercel/static" },
    { "src": "frontend/dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "frontend/api/$1" },
    { "src": "/assets/(.*)", "dest": "frontend/dist/assets/$1" },
    { "src": "/(.*)", "dest": "frontend/dist/index.html" }
  ]
}


